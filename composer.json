{
    "name": "ngsoft/url",
    "description": "esperecyan/url fork that uses a polyfill for intl and is compatible with psr-7 specifications.",
    "keywords": ["Web Standards", "WHATWG", "W3C", "URL", "URLSearchParams", "RFC 3986"],
    "homepage": "https://github.com/ngsoft/url",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "100の人",
            "homepage": "https://twitter.com/esperecyan",
            "role": "Developer"
        },
        {
            "name": "Aymeric Anger",
            "homepage": "https://github.com/ngsoft",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "esperecyan/webidl": "^2.0.0",
        "symfony/polyfill-mbstring": "^1.3.0",
        "symfony/intl": "^4.3",
        "ext-iconv": "*",
        "ext-pcre": "*",
        "ext-filter": "*",
        "ext-ctype": "*",
        "ext-json": "*",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.17",
        "apigen/apigen": "4.2.0-RC1 || ^4.2.0",
        "ext-spl": "*",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\URL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "esperecyan\\url\\": "tests/"
        }
    }
}
